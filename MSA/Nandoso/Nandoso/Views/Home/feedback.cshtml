@{
    ViewBag.Title = "Feedback";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>
<head>
    <title>Nandoso - Feedback</title>
    <meta charset="utf-8" />
    @*<link rel="stylesheet" href="../styles/style.css" />*@
    <link rel="stylesheet" href="../Content/bootstrap.min.css " />
</head>
<body>

    <img src="/img/chicken.jpg" style="width:427px;height:328px;">
    <div class="feedback">
        <div class="row">
            <div class="col-md-3 col-md-offset-2">

                <div class="feedbackform">
                    <h1>Feedback Form</h1>
                    <form action="index.html" method="post">
                        <p>First Name:</p>
                        <input type="text" name="name" />
                        <br />
                        <br />
                        <p>Email:</p>
                        <input type="email" name="email" />
                        <br />
                        <br />
                        <p>Your feedback</p>
                        <textarea name="feedback" rows="10" cols="30"></textarea><br />
                        <input type="submit" value="submit" />
                    </form>
                </div>
            </div>
            <div class="col-md-2 col-md-offset-2">
                <div class="contact">
                    <h1>Contact Us</h1>
                    <p><strong>Telephone:</strong> 0800-NANDOSO (0800-6263-676)</p>
                    <p><strong>Email:</strong>support@nandoso.com</p>
                    <p><strong>Address:</strong>Mt. Glen Eden, New Zealand</p>
                    <p><strong>Hours:</strong>11:00AM-7:30PM Mondays-Fridays</p>
                </div>
            </div>
        </div>
    </div>
</body>